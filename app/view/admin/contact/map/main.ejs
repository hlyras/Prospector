<%- include('./filter.ejs') %>

<script>
  document.addEventListener("DOMContentLoaded", async () => {
    contact_map_btn.addEventListener("click", async () => {
      lib.popup(await contactMapInterface(), null, true);

      let contact_map_options = {}
      contactMapFilter(contact_map_options);
    })
  });

  async function contactMapInterface() {
    let contact_map_container = lib.element.create("div", {
      id: "contact_map_container",
      class: "box a1 container h-center"
    });

    return contact_map_container;
  };


</script>