<script>
  function messageDiv(message) {
    let message_div = lib.element.create("div", {
      class: "box b1 container border-st right"
    });
    // message.from_me && lib.addCss(message_div, ["right"]);

    console.log(message);

    let text_content = lib.element.create("div", {
      class: "box b2-3 container wrap border-st"
    }, message.content);
    message_div.append(text_content);

    return message_div;
  };
</script>